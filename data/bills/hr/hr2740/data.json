  "introduced_at": "2015-06-11", 
  "number": "2740", 
  "official_title": "To prohibit the application of certain restrictive eligibility requirements to foreign nongovernmental organizations with respect to the provision of assistance under part I of the Foreign Assistance Act of 1961.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Global Democracy Promotion Act", 
  "sponsor": {
    "district": "17", 
    "name": "Lowey, Nita M.", 
    "state": "NY", 
    "thomas_id": "00709", 
    "title": "Rep", 
