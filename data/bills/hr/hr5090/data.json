  "introduced_at": "2016-04-28", 
  "number": "5090", 
  "official_title": "To ensure that air transportation between the United States and the European Union complies with the intent of article 17 bis of the United States-European Union-Norway-Iceland Air Transport Agreement of June 21, 2011.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "4", 
    "name": "DeFazio, Peter A.", 
    "state": "OR", 
    "thomas_id": "00279", 
    "title": "Rep", 
